<div class='list'>
<table>
  <tr>
    <th>Student ID#</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Login Number</th>
  </tr>

<% @students.each do |student| %>
  <tr>
    <td><%=h student.student_id %></td>
    <td><%=h student.first_name %></td>
    <td><%=h student.last_name %></td>
    <td><%=h student.login_id %></td>
  </tr>
<% end %>
</table>
</div>
