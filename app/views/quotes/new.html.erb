<div class='main'>
<h1>What do you have to say?</h1>

<% form_for(@quote) do |f| %>
  <%= f.error_messages %>

  <p>
    Student ID#<br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    Computer Login #<br />
    <%= f.text_field :login_id %>
  <p>
    Name<br />
    <%= f.text_field :name %>
  </p>
  <p>
    Email Address<br />
    <%= f.text_field :email %>*not required
  </p>
  <p>
    Your Quote<br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Submit!' %>
  </p>
<% end %>
</div>
